  border-radius: 5px;
  background-color: #11a2a7;
  color: white;
  display: block
}

#workout{
  text-transform: capitalize;
  margin: 56px auto;
  width: 80%;
  max-width: 360px;
  text-align: center;
}


#workout h4{
  margin: 18px 0
