}

.flex-container {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: space-around;
}
.controls{
  max-width: 660px;
  width: 60%;
  min-width: 240px;
}
.title, .overview, .warmup, .circuit, .cycle-wrapper{
  margin-bottom: 1em;
}
.exercise-wrapper, .overview-wrapper{
  margin: 1em auto;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: space-around;
}
.holder{
  display: block;
